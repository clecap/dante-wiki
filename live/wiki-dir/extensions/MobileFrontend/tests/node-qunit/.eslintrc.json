{
	"root": true,
	"extends": [
		"wikimedia/server",
		"wikimedia/mediawiki",
		"wikimedia/qunit"
	],
	"rules": {
		"compat/compat": "off",
		"computed-property-spacing": "off",
		"mediawiki/valid-package-file-require": "off",
		"no-underscore-dangle": "off",
		"no-use-before-define": "off",
		"strict": "off"
	},
	"env": {
		"browser": true
	}
}
